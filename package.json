{
  "name": "@superfluid-finance/tokenlist",
  "version": "2.0.5",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bootstrap": "ts-node --esm scripts/bootstrap.ts",
    "build:package": "rimraf sdk/dist && tsc -p ./tsconfig.package.json --outDir ./dist --declaration",
    "create:draft": "ts-node --esm scripts/create-draft.ts",
    "diff-lists": "ts-node --esm scripts/diff-lists.ts $1 $2 $3",
    "editor": "npx ts-node --esm ./editor/server.ts",
    "ipfs:publish": "ts-node --esm scripts/publish.ts $1",
    "update:version": "ts-node --esm scripts/update-version.ts $1",
    "superfluid-graphql-codegen": "graphclient build --dir ./subgraph"
  },
  "dependencies": {
    "@uniswap/token-lists": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^3.0.0",
    "@pinata/sdk": "^2.1.0",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.5",
    "@types/node-fetch": "^2.6.3",
    "@uniswap/token-list-bridge-utils": "^2.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^16.3.1",
    "graphql": "^16.7.1",
    "lodash": "^4.17.21",
    "node-fetch": "3.3.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vanilla-jsoneditor": "^0.17.8"
  },
  "packageManager": "pnpm@8.6.1"
}
